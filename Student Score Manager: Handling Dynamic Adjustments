let scores = [35, 75, 88, 92, 40, 30, 99, 60, 45, 55];

for (let i = 0; i < scores.length; i++) {
    if (scores[i] < 40) {
        scores[i] += 20;
    }
    if (scores[i] > 90) {
        scores[i] = 90;
    }
}

let passed = scores.filter(score => score >= 50).length;

console.log(passed);
console.log(scores);
